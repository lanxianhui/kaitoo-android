  <class name="com.timesoft.kaitoo.common.dto.User" table='"USER"'>
    <id column='"USER_ID"' name="id" type="long">
      <generator class="sequence">
        <param name="sequence">seq_user_id</param>
      </generator>
    </id>
    <property column='"EMAIL"' name="email"/>
    <property column='"PASSWORD"' name="password"/>
